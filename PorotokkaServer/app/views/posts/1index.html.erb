<!DOCTYPE html>
<html>
<% @post.each do |post| %>
  <br>
  <div><%= post.id %></div>
  <div><%= post.event_name %></div>
  <div><%= post.event_date %></div>
  <div><%= post.event_time %></div>
  <%= link_to "Edit", edit_post_path(post) %>
  <%= link_to "Delete", post_path(post), method: :delete %>

  <br>

<% end %>
</html>
